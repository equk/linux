update_interval 11
total_run_times 0
net_avg_samples 0
cpu_avg_samples 1

imlib_cache_size 0
double_buffer yes
no_buffers yes

format_human_readable

use_xft yes
xftfont profont:pixelsize=11
override_utf8_locale yes
text_buffer_size 256

own_window yes
own_window_type desktop
own_window_transparent yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager

alignment bottom_right

gap_x 10
gap_y 10

draw_shades no
draw_outline yes
default_outline_color 111111

default_color FFFFFF
color1 55ff55
color2 ce084e

TEXT
${color2}Kernel: ${color}${kernel}
${color2}Mem: ${color}${mem}${color2}/${color}${memmax}
${color2}Root: ${color}${fs_used /}
${color2}Home: ${color}${fs_used /home}
